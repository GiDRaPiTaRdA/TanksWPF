using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Tanks.Models.Fields
{
    public class Coodrinates
    {
        public Coodrinates(int x,int y)
        {
            this.X = x;
            this.Y = y;
        }

        public int X { get; }
        public int Y { get; }

        public override bool Equals(object obj)
        {
            return 
                ((Coodrinates)obj).X == this.X &&
                ((Coodrinates)obj).Y == this.Y;
        }

        public override string ToString()
        {
            return "X: " + this.X + " Y: " + this.Y;
        }
    }
}
